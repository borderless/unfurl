            var oldOnLoad = window.onload;
            window.onload = function() {
                if (typeof oldOnLoad === 'function') {
                    oldOnLoad();
                }
                if (window.Sailthru) {
                    Sailthru.setup({domain: 'sthorizon.khanacademy.org'});
                } 
            };
        })
    </script>
    
    

    
