            // Our scrolling destination depends on the elements' widths, but
            // styles are actually injected _after_ the initial mount, so we
            // won't get helpful width measurements until the next tick.
            setTimeout(function() {
                var container = document.querySelector(
                    "[data-tutorial-nav-container]");
                var row = container.querySelector("[data-tutorial-nav-row]");
                var selected = container.querySelector(
                    "[data-tutorial-nav-selected=true]");

                // Mark on the element that we set the position, so we don't do
                // it again in componentDidMount.
                row._dataScrollSet;

                var desiredPositionLeft =
                    container.offsetWidth / 2 - selected.offsetWidth / 2;

                // Scroll the row such that the selected item is centered
                row.scrollLeft = selected.offsetLeft - desiredPositionLeft;
            }, 0);
        </script></div></div></div></div><div class="page_njgkl1-o_O-pageWithTutorialNavIconsOnly_75j6io-o_O-pageWithTutorialNavOnSide_1ouwzwk" data-reactid=".19eo0q9htds.1"><div class="task-container" itemscope="" itemtype="http://schema.org/VideoObject" data-reactid=".19eo0q9htds.1.1"><meta itemprop="name" content="Bonds vs. stocks" data-reactid=".19eo0q9htds.1.1.0"/><meta itemprop="description" content="The difference between a bond and a stock." data-reactid=".19eo0q9htds.1.1.1"/><meta itemprop="uploadDate" content="2011-02-20T16:42:16Z" data-reactid=".19eo0q9htds.1.1.2"/><link itemprop="thumbnailUrl" href="https://cdn.kastatic.org/googleusercontent/4SKhQ_XNx98rrnAtDrvT3RnZKT-qCudwuhIfkKdirx5Rxl_34ULc5qSK3Q7WXVkQl9J3QXGq5c-J5n3waKg7T4IP" data-reactid=".19eo0q9htds.1.1.3"/><div class="videoBackdrop_15tgcpj" data-reactid=".19eo0q9htds.1.1.4"><div class="videoContainer_nj00a3" data-reactid=".19eo0q9htds.1.1.4.0"><div class="ka-video-player ka-video-player-economics-finance-domain" data-reactid=".19eo0q9htds.1.1.4.0.$rs1md3e4aYU"><span class="sr-only" data-reactid=".19eo0q9htds.1.1.4.0.$rs1md3e4aYU.0"><span data-reactid=".19eo0q9htds.1.1.4.0.$rs1md3e4aYU.0.0:0">Current time:</span><span data-reactid=".19eo0q9htds.1.1.4.0.$rs1md3e4aYU.0.1"> </span><span data-reactid=".19eo0q9htds.1.1.4.0.$rs1md3e4aYU.0.2">0:00</span><span data-reactid=".19eo0q9htds.1.1.4.0.$rs1md3e4aYU.0.3:0">Total duration:</span><span data-reactid=".19eo0q9htds.1.1.4.0.$rs1md3e4aYU.0.4"> </span><span data-reactid=".19eo0q9htds.1.1.4.0.$rs1md3e4aYU.0.5">9:21</span></span><div class="video-spacer-container" data-reactid=".19eo0q9htds.1.1.4.0.$rs1md3e4aYU.1"><div class="ka-video-player-container centered-when-windowed" data-reactid=".19eo0q9htds.1.1.4.0.$rs1md3e4aYU.1.0"><iframe class="player ka-player-iframe centered-when-windowed" id="video_rs1md3e4aYU" type="text/html" frameborder="0" allowfullscreen="" src="//www.youtube.com/embed/rs1md3e4aYU/?controls=1&amp;enablejsapi=1&amp;modestbranding=1&amp;