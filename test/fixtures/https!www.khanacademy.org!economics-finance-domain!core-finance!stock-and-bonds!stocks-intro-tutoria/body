            // Our scrolling destination depends on the elements' widths, but
            // styles are actually injected _after_ the initial mount, so we
            // won't get helpful width measurements until the next tick.
            setTimeout(function() {
                var container = document.querySelector(
                    "[data-tutorial-nav-container]");
                var row = container.querySelector("[data-tutorial-nav-row]");
                var selected = container.querySelector(
                    "[data-tutorial-nav-selected=true]");

                // Mark on the element that we set the position, so we don't do
                // it again in componentDidMount.
                row._dataScrollSet;

                var desiredPositionLeft =
                    container.offsetWidth / 2 - selected.offsetWidth / 2;

                // Scroll the row such that the selected item is centered
                row.scrollLeft = selected.offsetLeft - desiredPositionLeft;
            }, 0);
        </script></div></div></div><div class="discussion_alcbxv" data-reactid=".19eo0q9htds.1.1.9"><div class="wrapper_v0sft" data-reactid=".19eo0q9htds.1.1.9.0"><div class="bibliotron-discussion domain-economics-finance-domain discussion_113ell4" data-reactid=".19eo0q9htds.1.1.9.0.0"></div></div></div><div data-reactid=".19eo0q9htds.1.1.b"></div></div><div class="moduleList_p1diqp" data-reactid=".19eo0q9htds.1.2"><div class="moduleList_7ol0hf" data-reactid=".19eo0q9htds.1.2.0"><div class="module_823oet" id="up-next-module" data-reactid=".19eo0q9htds.1.2.0.$0"><div class="wide_i324i4" data-reactid=".19eo0q9htds.1.2.0.$0.0"><div style="float:left;" data-reactid=".19eo0q9htds.1.2.0.$0.0.0"><a href="/economics-finance-domain/core-finance/stock-and-bonds/stocks-intro-tutorial/v/what-it-means-to-buy-a-company-s-stock" class="link_1uvuyao" data-reactid=".19eo0q9htds.1.2.0.$0.0.0.0"><span data-reactid=".19eo0q9htds.1.2.0.$0.0.0.0.0"><span aria-hidden="true" class="chevron_126ds02-o_O-chevronLeft_g45as2" data-reactid=".19eo0q9htds.1.2.0.$0.0.0.0.0.0"><svg width="6.306900000000001" height="10" viewBox="0 0 63.069 100" data-reactid=".19eo0q9htds.1.2.0.$0.0.0.0.0.0.0"><path fill="#a75a05" d="M-.013 49.84q0-3.416 2.352-5.824l41.72-41.664q2.352-2.352 5.768-2.352t5.824 2.352l4.816 4.816q2.352 2.296 2.352 5.768t-2.352 5.824l-31.136 31.08 31.136 31.08q2.352 2.408 2.352 5.824t-2.352 5.768l-4.816 4.76q-2.296 2.352-5.768 2.352t-5.824-2.352l-41.72-41.664q-2.352-2.352-2.352-5.768z" data-reactid=".19eo0q9htds.1.2.0.$0.0.0.0.0.0.0.0"></path></svg></span><span data-reactid=".19eo0q9htds.1.2.0.$0.0.0.0.0.1">What it means to buy a company&#x27;s stock</span></span></a></div><div style="clear:both;" data-reactid=".19eo0q9htds.1.2.0.$0.0.2"></div></div></div></div></div></div></div></div>


